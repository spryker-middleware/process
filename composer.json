{
    "name": "spryker-middleware/process",
    "description": "Process module",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "spryker/console": "^4.0.0",
        "spryker/development": "^3.0.0",
        "spryker/transfer": "^3.4.0",
        "spryker/util-encoding": "^2.0.0",
        "spryker-middleware/logger": "dev-dev"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {
          "SprykerMiddleware\\": "src/SprykerMiddleware/"
        }
    },
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://code.spryker.com/repo/private"
        },
        {
            "type": "git",
            "url": "https://github.com/spryker-middleware/logger.git"
        }
    ]
}

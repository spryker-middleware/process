<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProcessSettings">
        <property name="name" type="string"/>
        <property name="iteratorSettings" type="IteratorSettings"/>
        <property name="aggregatorSettings" type="AggregatorSettings"/>
        <property name="loggerSettings" type="LoggerSettings"/>
    </transfer>

    <transfer name="IteratorSettings">
        <property name="offset" type="int"/>
        <property name="limit" type="int"/>
        <property name="parseAsArray" type="bool"/>
    </transfer>

    <transfer name="AggregatorSettings">
        <property name="threshold" type="int"/>
        <property name="writerConfig" type="WriterConfig"/>
    </transfer>

    <transfer name="WriterConfig">
        <property name="destination" type="string"/>
    </transfer>

    <transfer name="LoggerSettings">
        <property name="verboseLevel" type="int"/>
        <property name="isQuiet" type="bool"/>
    </transfer>

    <transfer name="MapperConfig">
        <property name="map" type="array"/>
        <property name="strategy" type="string"/>
    </transfer>

    <transfer name="TranslatorConfig">
        <property name="dictionary" type="array"/>
    </transfer>

</transfers>